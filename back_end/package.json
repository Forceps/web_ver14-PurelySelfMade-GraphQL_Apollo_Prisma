{
  "name": "back_end",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cd src && nodemon --exec ts-node server.ts -e ts,graphql,gql",
    "prism": "prisma introspect && prisma generate",
    "pretypes": "gql-merge --out-file ./src/GraphQL_API/LibForGQL/mergedSchema/types/schema.gql ./src/GraphQL_API/API/**/*.gql",
    "types": "graphql-to-typescript ./src/GraphQL_API/LibForGQL/mergedSchema/types/schema.gql ./src/GraphQL_API/LibForGQL/mergedSchema/types/graph.d.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "postbuild": "cd src && copy ./GraphQL_API/API/**/*.gql ../dist/GraphQL_API/API",
    "start": "cd dist && node ."
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/graphql": "^14.5.0",
    "@types/helmet": "^0.0.47",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.14",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/python-shell": "^0.4.2",
    "babel-runtime": "^6.26.0",
    "gql-merge": "^0.0.6",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@prisma/client": "^2.1.1",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^6.0.10",
    "graphql-yoga": "^1.18.3",
    "helmet": "^3.23.2",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "python": "^0.0.4",
    "python-shell": "^2.0.1"
  }
}
